{
  "name": "faunatils",
  "version": "0.2.1",
  "author": "Ondřej Kocián",
  "dependencies": {
    "dotenv": "^8.2.0",
    "faunadb": "^2.14.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/faunatils.esm.js",
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "typings": "dist/index.d.ts",
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
